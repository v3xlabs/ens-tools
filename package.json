{
    "name": "ens-tools",
    "description": "Opinionated ENS General Tooling",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "files": [
        "dist"
    ],
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ],
            "./react": [
                "./dist/react/index.d.ts"
            ],
            "./format": [
                "./dist/format/index.d.ts"
            ],
            "./chains": [
                "./dist/chains/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./react": {
            "import": "./dist/react/index.js",
            "require": "./dist/react/index.cjs",
            "types": "./dist/react/index.d.ts"
        },
        "./format": {
            "import": "./dist/format/index.js",
            "require": "./dist/format/index.cjs",
            "types": "./dist/format/index.d.ts"
        },
        "./chains": {
            "import": "./dist/chains/index.js",
            "require": "./dist/chains/index.cjs",
            "types": "./dist/chains/index.d.ts"
        }
    },
    "type": "module",
    "scripts": {
        "test": "jest",
        "lint": "eslint -c .eslintrc.json --ext .ts ./src",
        "pub": "pnpm publish --no-git-checks",
        "build": "tsup"
    },
    "keywords": [
        "ens",
        "tools",
        "ccip",
        "evm"
    ],
    "author": "V3X Labs",
    "license": "lgpl-3.0",
    "devDependencies": {
        "@jest/types": "^29.4.3",
        "@types/jest": "^29.4.0",
        "@types/react": "^18.2.0",
        "@typescript-eslint/parser": "^5.42.1",
        "@wagmi/core": "^0.10.10",
        "eslint": "^8.27.0",
        "eslint-plugin-v3xlabs": "^1.5.2",
        "ethers": "^5",
        "jest": "^29.4.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "swr": "^2.1.5",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tsup": "^6.5.0",
        "typescript": "^4.8.4",
        "vitest": "^0.31.0",
        "wagmi": "^0.12.12"
    },
    "peerDependencies": {
        "@wagmi/core": "^0.10.10",
        "ethers": "^5",
        "react": "^18.2.0",
        "swr": "^2.1.5",
        "viem": "^1.10.9",
        "wagmi": "^0.12.12"
    }
}
